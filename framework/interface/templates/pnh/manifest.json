{
  "toolName":"OWASP OWTF",
  "protocolVersion":"0.2",
  "features":{
    "proxy":{
      "PAC":"{{ pnh_url }}/proxy.pac",
      "CACert":"{{ pnh_url }}/ca.crt"
    },
    "commands":{
      "prefix":"owtf",
      "manifest":"{{ pnh_url }}/service.json"
    },
    "probe":{
      "transport":"HTTPMessageTransport",
      "endpoint":"{{ pnh_url }}/?apikey={{ api_key }}&",
      "oracle":"{{ pnh_url }}/?apikey={{ api_key }}&id=",
      "endpoint":"OWTF"
    }
  }
}